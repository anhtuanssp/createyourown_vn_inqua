{
  "name": "fieldmetrics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "config": {
    "postcss": "./node_modules/.bin/postcss",
    "gulp": "./node_modules/.bin/gulp",
    "concurrent": "./node_modules/.bin/concurrent"
  },
  "scripts": {
    "autoprefix-import-page": "postcss --use autoprefixer --output resources/css/sass/pages/import/import-data.css resources/css/sass/pages/import/import-data.css",
    "build-sass-import-page": "node-sass --output-style expanded --source-map true --output resources/css/sass/pages/import/ resources/css/sass/pages/import/import-data.scss --outFile resources/css/sass/pages/import/import-data.css",
    "prebuild-watch-import-page": "npm run build-sass-import-page",
    "build-watch-import-page": "npm run build-sass-import-page -- --watch",

    "autoprefix-example-page": "postcss --use autoprefixer --output resources/css/sass/application.css resources/css/sass/application.css",
    "build-sass-example-page": "node-sass --output-style expanded --source-map true resources/css/sass/pages/example/example.scss > node-sass resources/css/sass/pages/example/example.css",
    "prebuild-watch-example-page": "npm run build-sass-example-page",
    "build-watch-sass-example-page": "npm run build-sass-example-page -- --watch",

    "page1": "gulp --gulpfile app/pages/page1/gulpfile.js",
    "page2": "gulp --gulpfile app/pages/exmpleNg/gulpfile.js"
  },
  "keywords": [
    "fieldmetrics",
    "angular",
    "webpack",
    "es6",
    "ecmascript6"
  ],
  "author": "Posiba Front End Team",
  "license": "ISC",
  "devDependencies": {
    "angular": "^1.4.9",
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "concurrently": "^2.0.0",
    "file-loader": "^0.8.5",
    "gulp": "^3.9.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "lodash": "^4.5.0",
    "node-sass": "^3.4.2",
    "postcss-cli": "^2.5.1",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.12.14",
    "webpack-stream": "^3.1.0"
  }
}
